{
  "name": "jse",
  "displayName": "jse",
  "description": "jse",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.52.0"
  },
  "publisher": "@zstings",
  "repository": {
    "type": "git",
    "url": "https://github.com/zstings/jse.git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:typescript"
  ],
  "main": "./extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "properties": {
        "jse.isEval": {
          "type": "boolean",
          "default": true,
          "description": "是否开启eval模式的压缩"
        },
        "jse.identificationIdentifier": {
          "type": "string",
          "default": ".bak",
          "description": "识别标识符, 默认.bak, 源文件必须为.bak.js或者.bak.ts的后缀插件才会激活开启压缩"
        },
        "jse.generateIdentifier": {
          "type": "string",
          "default": ".min",
          "description": "生成标识符, 默认.min, 生成的文件会以.min.js结尾"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "pnpm run lint",
    "test": "vscode-test",
    "dev": "vite build -w",
    "build": "pnpm vsce package --no-dependencies"
  },
  "devDependencies": {
    "@types/vscode": "1.52.0",
    "@vscode/vsce": "^2.24.0"
  }
}
